{
  "name": "generator-html-project-boilerplate",
  "version": "0.0.0",
  "description": "A simple HTML CSS & JS Project boilerlate. Made by Mukitur Rahman Ashik",
  "homepage": "",
  "author": {
    "name": "Mukitur Rahman Ashik",
    "email": "gamerashik007@gmail.com",
    "url": "https://mukiturrahman.github.io/"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "",
    "yeoman-generator"
  ],
  "devDependencies": {
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "mukiturrahman/generator-html-project-boilerplate",
  "license": "ISC"
}
